{
    "name": "vuejs-datatable",
    "version": "1.7.0",
    "description": "A Vue.js component for filterable, sortable, and paginated tables.",
    "main": "dist/vuejs-datatable.esm.js",
    "module": "dist/vuejs-datatable.esm.js",
    "browser": "dist/vuejs-datatable.esm.js",
    "types": "dist/index-esm.d.ts",
    "scripts": {
        "doc": "npm run doc:examples && npm run doc:copy-assets && npm run doc:typedoc",
        "doc:copy-assets": "cp-cli dist/vuejs-datatable.js tutorials/assets/assets/vuejs-datatable.js",
        "doc:examples": "rimraf tutorials/builds && node --experimental-modules tutorials/build-examples.mjs",
        "doc:typedoc": "rimraf docs && typedoc --options ./typedoc.json",
        "test": "npm run test:unit && npm run test:integration",
        "test:ci": "jest --config jest.config.js",
        "test:ci:watch": "npm run test:ci --watch",
        "test:unit": "jest --config jest.unit.config.js",
        "test:unit:watch": "npm run test:unit -- --watch",
        "test:integration": "jest --config jest.integration.config.js",
        "test:integration:watch": "npm run test:integration -- --watch",
        "lint": "tslint --project tsconfig.json",
        "lint:fix": "tslint --fix --project tsconfig.json",
        "build": "npm run build:production",
        "build:dev": "rollup -c rollup.config.js",
        "build:production": "rollup -c rollup.config.js --environment BUILD:production",
        "build:watch": "rollup -c rollup.config.js --watch",
        "version": "npm run prepublishOnly && git add docs --force",
        "prepublishOnly": "npm run test && npm run build && npm run doc",
        "release": "standard-version -a",
        "prerelease": "standard-version -a --prerelease"
    },
    "keywords": [
        "Vue.js",
        "vue",
        "datatable",
        "table",
        "filter",
        "sort",
        "sortable",
        "paginate",
        "pagination"
    ],
    "files": [
        "types",
        "dist",
        "docs"
    ],
    "dependencies": {
        "core-js": "^3.0.1",
        "object-path": "^0.11.4",
        "vue-property-decorator": "^8.1.0"
    },
    "devDependencies": {
        "@babel/core": "^7.4.3",
        "@babel/preset-env": "^7.4.3",
        "@diaspora/dev-tslint": "^1.1.2",
        "@types/jest": "^24.0.11",
        "@types/object-path": "^0.11.0",
        "@vue/test-utils": "^1.0.0-beta.29",
        "axios": "^0.18.0",
        "babel-core": "^7.0.0-bridge.0",
        "babel-jest": "^24.7.1",
        "babel-preset-env": "^1.7.0",
        "cp-cli": "^2.0.0",
        "flush-promises": "^1.0.2",
        "jasmine-core": "^3.4.0",
        "jest": "^24.7.1",
        "jsdom": "^14.0.0",
        "karma": "^4.0.1",
        "karma-chrome-launcher": "^2.2.0",
        "karma-jasmine": "^2.0.1",
        "moment": "^2.24.0",
        "rimraf": "^2.6.3",
        "rollup": "^1.10.0",
        "rollup-plugin-babel": "^4.3.2",
        "rollup-plugin-commonjs": "^9.3.4",
        "rollup-plugin-jscc": "^1.0.0",
        "rollup-plugin-json": "^4.0.0",
        "rollup-plugin-license": "^0.8.1",
        "rollup-plugin-node-globals": "^1.4.0",
        "rollup-plugin-node-resolve": "^4.2.3",
        "rollup-plugin-replace": "^2.2.0",
        "rollup-plugin-terser": "^4.0.4",
        "rollup-plugin-typescript2": "^0.20.1",
        "rollup-plugin-visualizer": "^1.1.1",
        "rollup-plugin-vue-template-compiler": "git+https://github.com/GerkinDev/rollup-plugin-vue-template-compiler.git",
        "standard-version": "^5.0.2",
        "ts-jest": "^24.0.2",
        "tslint": "^5.15.0",
        "tslint-eslint-rules": "^5.4.0",
        "typedoc": "^0.14.2",
        "typescript": "^3.4.3",
        "vue": "^2.6.10",
        "vue-jest": "^3.0.4",
        "vue-template-compiler": "^2.6.10"
    },
    "peerDependencies": {
        "vue": "^2.6.10"
    },
    "standard-version": {
        "scripts": {
            "postbump": "npm run version"
        }
    },
    "author": {
        "name": "Patrick Stephan",
        "email": "pstephan1187@gmail.com"
    },
    "contributors": [
        {
            "name": "GerkinDev",
            "email": "agermain@ithoughts.io",
            "url": "https://ithoughts.io/"
        }
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/GerkinDev/vue-datatable"
    },
    "license": "MIT",
    "jest": {
        "preset": "ts-jest/presets/js-with-ts",
        "moduleFileExtensions": [ "js", "ts", "vue" ],
        "transform": {
            ".*\\.ts$": "ts-jest",
            ".*\\.html$": "<rootDir>/__tests__/helpers/transform-vue-template.js"
        },
        "collectCoverageFrom": [
            "src/**/*.{[jt]s,vue}",
            "!src/index-{esm,iife}.ts",
            "!**/node_modules/**"
        ],
        "collectCoverage": true,
        "globals": {
            "ts-jest": {
                "diagnostics": false
            }
        }
    }
}
